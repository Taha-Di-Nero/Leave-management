{
  "name": "coverage",
  "version": "1.3.0",
  "license": "MIT",
  "scripts": {
    "activateWorkerService": "ng set apps.0.serviceWorker=true",
    "desactivateWorkerService": "ng set apps.0.serviceWorker=false",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --show-circular-dependencies",
    "buildNoProgress": "ng build --show-circular-dependencies --no-progress",
    "prod": "ng build --prod --aot --extract-licenses --build-optimizer --show-circular-dependencies --base-href ./",
    "test": "ng test --sourcemaps=false",
    "cov": "ng test --code-coverage --sourcemaps=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "npm run activateWorkerService && npm run prod && npm run desactivateWorkerService"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.2",
    "@angular/cdk": "^5.0.2",
    "@angular/common": "^5.1.2",
    "@angular/compiler": "^5.1.2",
    "@angular/core": "^5.1.2",
    "@angular/forms": "^5.1.2",
    "@angular/material": "^5.0.2",
    "@angular/platform-browser": "^5.1.2",
    "@angular/platform-browser-dynamic": "^5.1.2",
    "@angular/router": "^5.1.2",
    "@angular/service-worker": "^5.1.2",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.8",
    "@swimlane/ngx-charts": "^7.0.1",
    "angular-calendar": "^0.22.1",
    "angular-confirmation-popover": "^3.4.1",
    "bootstrap": "^4.0.0-beta.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.3",
    "d3": "^4.12.0",
    "date-fns": "^1.29.0",
    "file-saver": "^1.3.3",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1",
    "ng-block-ui": "^1.0.0-beta.16",
    "ngx-toastr": "^7.1.0",
    "popper.js": "^1.13.0",
    "rxjs": "^5.5.5",
    "tether": "^1.4.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.2",
    "@angular/compiler-cli": "^5.1.2",
    "@angular/language-service": "^5.1.2",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "^2.8.2",
    "@types/node": "^8.5.2",
    "codelyzer": "^4.0.2",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.7.2",
    "protractor": "^5.2.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  }
}
